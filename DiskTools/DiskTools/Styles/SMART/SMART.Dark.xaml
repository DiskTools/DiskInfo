<ResourceDictionary x:Name="GaugeDarkDictionary"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="using:DiskInfo.Controls"
                    xmlns:converters="using:DiskInfo.Converters"
                    xmlns:DTR="using:DiskInfo.Helpers.DynamicThemeResource">
    <SolidColorBrush x:Key="MyBrush0" Color="White" />
    <LinearGradientBrush x:Key="MyBrush" StartPoint="0,0" EndPoint="1.6,1">
        <GradientStop x:Name="GradientStop9" Offset="1" Color="Transparent" />
        <GradientStop x:Name="GradientStop10" Offset="0" Color="White" />
    </LinearGradientBrush>
</ResourceDictionary>
